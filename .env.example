# API Keys (Required)
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Override default models
# OPENAI_MODEL=gpt-4o-mini      # Recommended: Fast & cheap
# OPENAI_MODEL=gpt-3.5-turbo    # Cheaper but lower quality
# OPENAI_MODEL=gpt-4o           # Best quality, more expensive

# GEMINI_MODEL=gemini-2.5-flash       # Recommended: Best for video analysis with reasoning
# GEMINI_MODEL=gemini-2.5-flash-lite  # Fastest & cheapest, limited reasoning
# GEMINI_MODEL=gemini-1.5-flash       # Older model, less capable

# Flask Configuration
SECRET_KEY=your-secret-key-change-in-production
FLASK_ENV=development

# Redis (Optional - falls back to in-memory cache if not available)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379/0

# Processing Configuration (Optional)
MAX_VIDEO_DURATION=3600  # Maximum video duration in seconds (1 hour)
FRAME_SAMPLE_RATE=1      # Frames per second to extract
BATCH_SIZE=32
MAX_FRAMES=100          # Maximum number of frames to extract

# CORS (Optional)
CORS_ORIGINS=http://localhost:3000

# Server Ports (Optional)
BACKEND_PORT=5001  # Using 5001 because macOS Control Center uses 5000
FRONTEND_PORT=3000
